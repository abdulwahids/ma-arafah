name = amino acids
biological activity = IC50

# input data
molecules directories = test-data/amino-acids
molecule names file = test-data/amino-acids-names.txt
used descriptors file = test-data/amino-acids-descriptors.txt

### SOM field viewers ###
show opengl viewer = false #need -Djava.library.path=/path/to/jogl/lib
write cluster mcss = false
write cluster info = true
cluster mcss directory = test-data/amino-acids-mcss
cluster info directory = test-data/amino-acids-mcss
cluster prefix = clust-
mcss prefix = aa-mcss-
mcss format = smiles

# SOM initialization
field length = 10
field width = 10

# SOM functions
max iterations = 1

verbosity = Verbose #options are: VeryVerbose, Everything